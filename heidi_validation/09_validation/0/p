{

	"(inlet|outlet|sides)"
    {
        // type            freestreamPressure;
        // freestreamValue uniform $pOut;
		type            freestream;
        freestreamValue uniform $pOut;
        value           uniform $pOut;
    }

    rocket
    {
		type			zeroGradient;
    }
