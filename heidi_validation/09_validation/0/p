{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

pOut			95540;

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform $pOut;

