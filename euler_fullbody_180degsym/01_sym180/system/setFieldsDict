FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volVectorFieldValue U (0 0 0)
    volScalarFieldValue T 300
    volScalarFieldValue p 100000
);

regions
(
    boxToCell
    {
        box (-1 0.5 -4.7) (1 1 1);
        fieldValues
        (
            volVectorFieldValue U (0 0 -10)
        );
    }
);


